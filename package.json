{
  "name": "Stride",
  "version": "2.0.0",
  "description": "Boilerplate/framework for developing APIs quickly and effectively in node.",
  "private": true,
  "main": "./build/server.js",
  "files": [
    "build",
    "src",
    "test"
  ],
  "scripts": {
    "start": "node ./build/server.js",
    "build": "npx gulp build",
    "dev": "npx nodemon --exec npx babel-node src/server.js",
    "pretest": "npx eslint --ignore-path .gitignore .",
    "test": "npx mocha ./test/main.js --require @babel/register",
    "mogront": "npx babel-node ./node_modules/mogront/build/bin/cli.js --config=\"src/config/database.js\" --template=\"es6\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:KieronWiltshire/Stride.git"
  },
  "author": "Kieron Wiltshire",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KieronWiltshire/Stride/issues"
  },
  "homepage": "https://github.com/KieronWiltshire/Stride",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-root-import": "^6.4.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "mocha": "^7.0.1",
    "mogront": "^1.3.2",
    "node-mocks-http": "^1.8.1",
    "nodemon": "^2.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "ignore": [
      "node_modules"
    ],
    "plugins": [
      "add-module-exports",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-transform-classes",
      [
        "babel-plugin-root-import",
        {
          "rootPathSuffix": "src"
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "body-parser": "^1.19.0",
    "bootit": "^1.3.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-parse-variables": "^0.2.3",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "greenlock": "^4.0.4",
    "helmet": "^3.21.2",
    "method-override": "^3.0.0",
    "mongodb": "^3.5.3",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.3",
    "respondent": "^1.1.2",
    "socket.io": "^2.4.0"
  }
}
