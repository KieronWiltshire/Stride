{
  "name": "stride",
  "version": "1.0.0",
  "description": "Boilerplate/framework for developing APIs quickly and effectively in node",
  "private": true,
  "main": "./build/server.js",
  "files": [
    "build",
    "src",
    "test"
  ],
  "scripts": {
    "start": "node ./build/server.js",
    "build": "npx gulp build",
    "dev": "npx nodemon --exec npx babel-node src/server.js",
    "pretest": "npx eslint --ignore-path .gitignore .",
    "test": "npx mocha ./test/main.js --require @babel/register",
    "mogront": "npx babel-node ./node_modules/mogront/build/bin/cli.js --config=\"mogront.config.js\" --template=\"es6\""
  },
  "author": "Kieron Wiltshire",
  "license": "See license.md",
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "bootit": "^1.3.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "dotenv-parse-variables": "^0.2.0",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "gelert": "^2.0.0",
    "greenlock": "^2.6.7",
    "helmet": "^3.15.1",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.13",
    "method-override": "^3.0.0",
    "mogront": "^1.3.0",
    "mongodb": "^3.1.13",
    "morgan": "^1.9.1",
    "nodemailer": "^5.1.1",
    "npm": "^6.12.1",
    "pug": "^2.0.3",
    "randomstring": "^1.1.5",
    "respondent": "^1.1.1",
    "socket.io": "^2.2.0",
    "uuid": "^3.3.2",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-async-to-generator": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "eslint": "^5.14.1",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "mocha": "^6.0.2",
    "node-mocks-http": "^1.7.3",
    "nodemon": "^1.18.10"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "ignore": [
      "node_modules"
    ],
    "plugins": [
      "add-module-exports",
      "@babel/plugin-transform-async-to-generator",
      "transform-class-properties",
      "transform-export-extensions",
      "@babel/plugin-transform-runtime",
      [
        "babel-plugin-transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ],
      [
        "babel-plugin-root-import",
        {
          "rootPathSuffix": "src"
        }
      ]
    ]
  }
}
